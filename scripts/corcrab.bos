}

FireTertiary()
{
	if( Static_Var_5 <= 0 )
	{
		if( gun_3 == 1 )
		{
			emit-sfx 259 from tube1;
			sleep 200;
		}
		if( gun_3 == 2 )
		{
			emit-sfx 259 from tube2;
			sleep 200;
		}
		if( gun_3 == 3 )
		{
			emit-sfx 259 from tube3;
			sleep 200;
		}
		++gun_3;
		if( gun_3 == 4 )
		{
			gun_3 = sstoparm2;
		}
	}
}

