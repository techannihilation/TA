}

AimTertiary(heading, pitch)
{
	if( Static_Var_5 <= 0 )
	{
		start-script RequestState(0);
		signal SIG_AIM_3;
		set-signal-mask SIG_AIM_3;
		turn turret1 to y-axis heading speed <12.000000>;
