{
	bMoving = FALSE;
	bAiming = FALSE;
	Static_Var_3 = 0;
	Static_Var_4 = 1;
	Static_Var_1 = 0;
	hide flash;
	restore_delay = 2000;
	Static_Var_6 = 1;
	Static_Var_7 = 0;
	Static_Var_8 = get MAX_SPEED;
	Static_Var_9 = 100;
	start-script SmokeUnit();
