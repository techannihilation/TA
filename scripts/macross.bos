MotionControl(Func_Var_1, Func_Var_2)
{
	while( TRUE )
	{
		Func_Var_1 = bMoving;
		Func_Var_2 = Static_Var_2;
		if( Func_Var_1 )
		{
			if( Func_Var_2 )
			{
				Static_Var_3 = 1;
				call-script walklegs2();
			}
			if( !Func_Var_2 )
			{
				Static_Var_3 = 0;
				call-script Walk2();
			}
		}
		if( !Func_Var_1 )
		{
			Static_Var_3 = 1;
			call-script stand();
		}
	}
}

Create()
{
	hide flare1;
	hide flare2;
	gun_4 = 0;
	gun_3 = luparm;
	gun_2 = luparm;
	Static_Var_6 = 1;
	dont-cache head;
	bMoving = FALSE;
	Static_Var_2 = 0;
	Static_Var_3 = 1;
	turn rlowarm to x-axis <-20.000000> now;
	turn llowarm to x-axis <-20.000000> now;
	restore_delay = 3000;
	start-script MotionControl();
}

StartMoving()
{
	bMoving = TRUE;
}

StopMoving()
{
	bMoving = FALSE;
}

