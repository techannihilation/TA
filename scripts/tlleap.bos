QueryNanoPiece(piecenum)
{
	if( spray == 1 )
	{
		piecenum = rfnano;
	}
	if( spray == 2 )
	{
		piecenum = lfnano;
	}
	if( spray == 3 )
	{
		piecenum = rbnano;
	}
	if( spray == 4 )
	{
		piecenum = lbnano;
	}
	++spray;
	if( spray > 4 )
	{
		spray = 1;
	}
}

Activate()
{
	signal SIG_ACTIVATE;
	start-script RequestState(0);
}

Deactivate()
{
	signal SIG_ACTIVATE;
	set-signal-mask SIG_ACTIVATE;
	if( !Static_Var_2 )
	{
		sleep 5000;
	}
	set-signal-mask 0;
	start-script RequestState(1);
}

