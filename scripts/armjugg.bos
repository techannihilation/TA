		move lhbarrel to z-axis [0.000000] speed [60.000000];
	}
	++gun_2;
	if( gun_2 == 2 )
	{
		gun_2 = head;
	}
}

FireTertiary()
{
	++gun_3;
	if( gun_3 == 2 )
	{
		gun_3 = head;
	}
}

