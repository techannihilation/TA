	hide r2Blitz;
	hide lRakBlitz;
	hide rRakBlitz;
	hide lmRakWerf;
	hide rmRakWerf;
	dont-cache lBlinker;
	dont-shade lBlinker;
	dont-cache rBlinker;
	dont-shade rBlinker;
	turn Stange to x-axis <-64.000000> now;
	Static_Var_10 = 4;
	unitviewer = FALSE;
	bMoving = FALSE;
	bAiming = FALSE;
	gun_1 = Stange;
	gun_3 = Huefte;
	Static_Var_3 = 1;
	Static_Var_6 = 0;
	Static_Var_9 = get HEALTH;
	spin RauchArm around y-axis speed <300.000000>;
	set ARMORED to 1;
	start-script SmokeUnit();
}

