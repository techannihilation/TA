		emit-sfx 1025 from blink11;
		emit-sfx 1026 from blink7;
		sleep 1000;
		emit-sfx 1025 from blink2;
		emit-sfx 1026 from blink7;
		sleep 1000;
	}
	sleep 50;
	start-script Standby();
}

