	if( severity <= 25 )
	{
		corpsetype = 1;
		explode arm1a type NOHEATCLOUD | BITMAPONLY | BITMAP1;
		explode arm1b type NOHEATCLOUD | BITMAPONLY | BITMAP2;
		explode arm2a type NOHEATCLOUD | BITMAPONLY | BITMAP3;
		explode arm2b type NOHEATCLOUD | BITMAPONLY | BITMAP4;
		explode arm3a type NOHEATCLOUD | BITMAPONLY | BITMAP5;
		explode arm3b type NOHEATCLOUD | BITMAPONLY | BITMAP1;
		explode arm4a type NOHEATCLOUD | BITMAPONLY | BITMAP2;
		explode arm4b type NOHEATCLOUD | BITMAPONLY | BITMAP3;
		explode base type NOHEATCLOUD | BITMAPONLY | BITMAP4;
