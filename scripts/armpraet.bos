Create()
{
	hide hflare;
	hide lgunflare;
	hide rgunflare;
	bMoving = FALSE;
	bAiming = FALSE;
	Static_Var_5 = 1;
	gun_1 = head;
	gun_3 = head;
	restore_delay = 2000;
	start-script MotionControl();
	start-script SmokeUnit();
	shieldonoff = 1;
	
}

